Template.Loader({['WaitTime'] = 50,['DisableSeek'] = true,['DisableFigure'] = true,['RoomMax'] = 100,['RoomMin'] = 0},function()

local Spawner = loadstring(game:HttpGet('https://raw.githubusercontent.com/MuhXd/DoorSuff/main/OtherSuff/DoorEntitySpawn%2BSource'))()

-- Create entity

local entity = Spawner.createEntity({

    CustomName = "Billy", -- Custom name of your entity

    Model = "rbxassetid://13409300445", -- Can be GitHub file or rbxassetid

    Speed = 1500, -- Percentage, 100 = default Rush speed

    DelayTime = 5, -- Time before starting cycles (seconds)

    HeightOffset = 0,

    CanKill = true,

    NoDieOnCrouching = false,

    NoHiding = false,

    AntiCrucifix = false,

    KillRange = 100,

    OneRoom = false,

    DieOnLook = false,

    BreakLights = false,

    BackwardsMovement = false,

     MovementDeath = {

        true, -- Turned On?

        '1',  --- '1'= 'Instant Without Being Looked out' | '2' = 'With Being Looked At'

    },

    FlickerLights = {

        false, -- Enabled/Disabled

        4, -- Time (seconds)

    },

EntitySounds = { -- Can Get rid Of

PlaySound = {

              "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyidle.mp3?raw=true", -- SoundId Link or Roblox ID

            { Volume = 5 }, -- Sound properties

       },

        

                   

          Footsteps = {

             "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyidle.mp3?raw=true", -- SoundId Link or Roblox ID

              { Volume = 5 }, -- Sound properties

          },

    }, -- Up to Here

    Cycles = {

        Min = 100,

        Max = 100,

        WaitTime = 5,

    },

    CamShake = {

        true, -- Enabled/Disabled

        {100, 1000, 100, 100}, -- Shake values (don't change if you don't know)

        100, -- Shake start distance (from Entity to you)

    },

    Jumpscare = {

        true, -- Enabled/Disabled

        {

            Type = "3", -- "Normal" or 1 | "Pop" or 2 | "endlessdoorsrebound" or "Rebound" or 3 | More coming Soon

            Image1 = "rbxassetid://13173976967", -- Image1 url

            Image2 = "rbxassetid://13173976967", -- Image2 url

            Shake = true,

            Sound1 = {

                "https://github.com/JTheBestGamer/jai-hub/blob/sounds/imaginedyingtobillydeath.mp3?raw=true", -- SoundId Link or Roblox ID

                { Volume = 5 }, -- Sound properties

            },

            Sound2 = {

                "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyjumpscare.mp3?raw=true", -- SoundId Link or Roblox ID

                { Volume = 10 }, -- Sound properties

            },

            Flashing = {

                true, -- Enabled/Disabled

                Color3.fromRGB(0, 0, 255), -- Color

            },

            Tease = {

                true, -- Enabled/Disabled

                Min = 8,

                Max = 8,

            },

        },

    },

    Color = 'CuriousLight', -- CuriousLight ( Yellow ) | GuidingLight ( Blue )

    DiffrentMessages = true,

    CustomDialog = {  

        {"You died to Billy.", "You have a skill issue."},

        {"Remember your last experience(s).", "There is no hope, I cannot help with skill issue."},

        {"Bruh.", "You suck!"},

        {"It seems like Billy is causing quite the ruckus...", "Do i care?"},

         {"You suck lol.", "YOU HAVE A SKILL ISSUE ðŸ’€"}

    }

})

-----[[ Advanced Sctipting ]]-----

entity.Debug.OnEntityMoving = function(Invincible,Hiding,plrCollisionPoint)

print("Invincible: "..tostring(Invincible))

print("Player to Entity Collision (None hiding Point): "..tostring(plrCollisionPoint))

print("Hiding: "..tostring(Hiding))

end

       

entity.Debug.OnEntitySpawned = function()

    print("Entity has spawned:")

end

entity.Debug.OnEntityDespawned = function()

    print("Entity has despawned:")

end

entity.Debug.OnEntityStartMoving = function()

    print("Entity has started moving:")

end

entity.Debug.OnEntityFinishedRebound = function()

    print("Entity has finished rebound:")

end

entity.Debug.OnEntityEnteredRoom = function(entityTable, room)

    print("Entity:", "has entered room:",room)

end

entity.Debug.OnLookAtEntity = function()

    print("Player has looked at entity:")

end

entity.Debug.OnDeath = function()

    warn("Player has died.")

end

------------------------

-- Run the created entity

Spawner.runEntity(entity)

firesignal(game:GetService("ReplicatedStorage").EntityInfo.Caption.OnClientEvent, 'cool warning for the fact that billy is coming',true,5)

end)

Template.Loader({['WaitTime'] = 50,['DisableSeek'] = true,['DisableFigure'] = true,['RoomMax'] = 100,['RoomMin'] = 0},function()

local Spawner = loadstring(game:HttpGet('https://raw.githubusercontent.com/MuhXd/DoorSuff/main/OtherSuff/DoorEntitySpawn%2BSource'))()

-- Create entity

local entity = Spawner.createEntity({

    CustomName = "Billy", -- Custom name of your entity

    Model = "rbxassetid://13409300445", -- Can be GitHub file or rbxassetid

    Speed = 1500, -- Percentage, 100 = default Rush speed

    DelayTime = 5, -- Time before starting cycles (seconds)

    HeightOffset = 0,

    CanKill = true,

    NoDieOnCrouching = false,

    NoHiding = false,

    AntiCrucifix = false,

    KillRange = 100,

    OneRoom = false,

    DieOnLook = false,

    BreakLights = false,

    BackwardsMovement = false,

     MovementDeath = {

        true, -- Turned On?

        '1',  --- '1'= 'Instant Without Being Looked out' | '2' = 'With Being Looked At'

    },

    FlickerLights = {

        false, -- Enabled/Disabled

        4, -- Time (seconds)

    },

EntitySounds = { -- Can Get rid Of

PlaySound = {

              "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyidle.mp3?raw=true", -- SoundId Link or Roblox ID

            { Volume = 5 }, -- Sound properties

       },

        

                   

          Footsteps = {

             "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyidle.mp3?raw=true", -- SoundId Link or Roblox ID

              { Volume = 5 }, -- Sound properties

          },

    }, -- Up to Here

    Cycles = {

        Min = 100,

        Max = 100,

        WaitTime = 5,

    },

    CamShake = {

        true, -- Enabled/Disabled

        {100, 1000, 100, 100}, -- Shake values (don't change if you don't know)

        100, -- Shake start distance (from Entity to you)

    },

    Jumpscare = {

        true, -- Enabled/Disabled

        {

            Type = "3", -- "Normal" or 1 | "Pop" or 2 | "endlessdoorsrebound" or "Rebound" or 3 | More coming Soon

            Image1 = "rbxassetid://13173976967", -- Image1 url

            Image2 = "rbxassetid://13173976967", -- Image2 url

            Shake = true,

            Sound1 = {

                "https://github.com/JTheBestGamer/jai-hub/blob/sounds/onetwobucklemyshoebillydeath.mp3?raw=true", -- SoundId Link or Roblox ID

                { Volume = 5 }, -- Sound properties

            },

            Sound2 = {

                "https://github.com/JTheBestGamer/jai-hub/blob/sounds/billyjumpscare.mp3?raw=true", -- SoundId Link or Roblox ID

                { Volume = 10 }, -- Sound properties

            },

            Flashing = {

                true, -- Enabled/Disabled

                Color3.fromRGB(0, 0, 255), -- Color

            },

            Tease = {

                true, -- Enabled/Disabled

                Min = 5,

                Max = 5,

            },

        },

    },

    Color = 'CuriousLight', -- CuriousLight ( Yellow ) | GuidingLight ( Blue )

    DiffrentMessages = true,

    CustomDialog = {  

        {"You died to Billy.", "You have a skill issue."},

        {"Remember your last experience(s).", "There is no hope, I cannot help with skill issue."},

        {"Bruh.", "You suck!"},

        {"It seems like Billy is causing quite the ruckus...", "Do i care?"},

         {"You suck lol.", "YOU HAVE A SKILL ISSUE ðŸ’€"}

    }

})

-----[[ Advanced Sctipting ]]-----

entity.Debug.OnEntityMoving = function(Invincible,Hiding,plrCollisionPoint)

print("Invincible: "..tostring(Invincible))

print("Player to Entity Collision (None hiding Point): "..tostring(plrCollisionPoint))

print("Hiding: "..tostring(Hiding))

end

       

entity.Debug.OnEntitySpawned = function()

    print("Entity has spawned:")

